<%
  service = Linkables.new(service)
  values = values.map { |content_id| service.by_content_id(content_id) }
%>
<% if values.any? %>
  <ul class="govuk-list">
    <% values.each do |value| %>
      <li><%= value&.fetch("internal_name") %></li>
    <% end %>
  </ul>
<% else %>
  <%= t("documents.show.tags.none") %>
<% end %>
