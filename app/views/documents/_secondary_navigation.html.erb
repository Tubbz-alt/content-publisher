<%= render "components/secondary_navigation", {
  aria_label: "Document",
  items: [
    {
      label: t("documents.show.heading"),
      href: document_path(@edition.document),
      current: current_page?(document_path(@edition.document)),
      data_attributes: {
        gtm: "summary-nav"
      }
    },
    {
      label: t("documents.history.heading"),
      href: document_history_path(@edition.document),
      current: current_page?(document_history_path(@edition.document)),
      data_attributes: {
        gtm: "history-nav"
      }
    }
  ]
} %>
